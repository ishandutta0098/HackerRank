   "source": [
    "n = int(input())\n",
    "total = dict()\n",
    "for i in range(0, n):\n",
    "    tokens = input().split()\n",
    "    name = tokens[0]\n",
    "    total[name] = float(tokens[1]) + float(tokens[2]) + float(tokens[3])\n",
    "\n",
    "student = input()\n",
    "print (\"{0:.2f}\".format(total[student] / 3))"
   ]
  }
