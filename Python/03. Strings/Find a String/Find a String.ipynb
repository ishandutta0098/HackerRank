   "source": [
    "def count_substring(string, sub_string):\n",
    " results = 0\n",
    " sub_len = len(sub_string)\n",
    " for i in range(len(string)):\n",
    "    if string[i:i+sub_len] == sub_string:\n",
    "        results += 1\n",
    "\n",
    " return results"
   ]
  }
